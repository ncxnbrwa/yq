<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tl="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.iimedia.yq.MainActivity">

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="45dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="bottom"
        android:layout_marginBottom="44dp"
        android:background="@color/separate_view_color" />

    <com.flyco.tablayout.CommonTabLayout
        android:id="@+id/tabs"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_gravity="bottom"
        android:background="@android:color/white"
        tl:tl_iconHeight="24dp"
        tl:tl_iconWidth="24dp"
        tl:tl_indicator_color="@color/colorPrimaryDark"
        tl:tl_indicator_height="0dp"
        tl:tl_textSelectColor="@color/colorPrimaryDark"
        tl:tl_textUnselectColor="@color/colorPrimary"
        tl:tl_textsize="9sp"
        tl:tl_underline_color="@color/colorPrimaryDark"
        tl:tl_underline_height="0dp" />

</FrameLayout>
